{
  "name": "slparse",
  "version": "0.0.1",
  "description": "Retrieve slack bugs",
  "author": "matt-oconnell @matt-oconnell",
  "bin": {
    "slparse": "./bin/run"
  },
  "homepage": "https://github.com/matt-oconnell/slparse",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "matt-oconnell/slparse",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@slack/bolt": "^3.12.1",
    "@slack/web-api": "^6.6.0",
    "chalk": "^4.0.0",
    "common-errors": "^1.2.0",
    "conf": "^10.1.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/common-errors": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "tempy": "^2.0.0",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "slparse",
    "dirname": "slparse",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/matt-oconnell/slparse/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
